<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<VBox alignment="CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="pl.edu.pwr.pkuchnowski.statki.World.WorldController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <GridPane fx:id="map" alignment="CENTER" maxHeight="400" maxWidth="400" minHeight="400" minWidth="400" />
    <Label fx:id="welcomeText"/>
    <Button fx:id="startButton" text="Start" onAction="#onStartButtonClick"/>
</VBox>
